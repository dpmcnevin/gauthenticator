<h1>Listing codes</h1>

<table>
  <tr>
    <th>Account</th>
    <th>Key</th>
    <th>Current</th>
    <th></th>
  </tr>

<% @codes.each do |code| %>
  <tr>
    <td><%= link_to code.account, code %></td>
    <td><%= code.key %></td>
    <td><%= code.current_token %></td>
    <td><%= link_to 'Destroy', code, confirm: 'Are you sure?', method: :delete %></td>
  </tr>
<% end %>
</table>

<br />

<%= link_to 'New Code', new_code_path %>
